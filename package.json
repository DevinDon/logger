{
  "name": "@iinfinity/logger",
  "version": "0.3.4",
  "description": "TypeScript / JavaScript colorful logger, support saving logs to file(s).",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs",
    "example": "src/demo",
    "test": "src/test"
  },
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "sh -c 'rm -rf dist out temp'",
    "debug": "node --inspect-brk -r ts-node/register src/main/index.ts",
    "demo": "node -r ts-node/register src/demo",
    "prepublishOnly": "npm run clean && npm run build",
    "start": "node dist",
    "test": "node node_modules/jasmine/bin/jasmine.js --config=src/test/jasmine.json --reporter=jasmine-console-reporter --require=ts-node/register",
    "watch": "tsc -w"
  },
  "keywords": [
    "typescript",
    "color",
    "console",
    "log",
    "logger"
  ],
  "author": "IInfinity <I.INF@Outlook.com> (https://blog.don.red/)",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/jasmine": "3.6.4",
    "@types/node": "14.14.31",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "eslint": "7.21.0",
    "jasmine": "3.6.4",
    "jasmine-console-reporter": "3.1.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevinDon/logger.git"
  },
  "bugs": {
    "url": "https://github.com/DevinDon/logger/issues"
  },
  "homepage": "https://github.com/DevinDon/logger"
}
